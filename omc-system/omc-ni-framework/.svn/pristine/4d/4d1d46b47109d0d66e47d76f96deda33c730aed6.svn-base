<service-configuration>
  <service>
    <name>Start WebServer</name>
    <class-name>com.ltln.netmgt.server.util.StartWebContainer</class-name>
  </service>
  <service>
    <name>Authorization Management</name>
    <class-name>com.ltln.netmgt.server.security.authorization.NmsAuthManager</class-name>
  </service>
  <service>
    <name>Topo Management</name>
    <class-name>com.ltln.netmgt.server.topodb.DBServer</class-name>
  </service>
  <service>
    <name>Device Management</name>
    <class-name>com.ltln.netmgt.server.ms.NMSMServer</class-name>
  </service>
  <service>
    <name>Map Server</name>
    <class-name>com.ltln.netmgt.server.mapdb.MapServerBE</class-name>
