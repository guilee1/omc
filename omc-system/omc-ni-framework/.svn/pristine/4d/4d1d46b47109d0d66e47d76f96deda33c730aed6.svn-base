    <name>Scheduler Management</name>
    <class-name>com.ltln.netmgt.server.scheduler.NmsPolicyMgr</class-name>
  </service>
  <service>
    <name>System Monitor</name>
    <class-name>com.ltln.netmgt.server.startnms.AdminModule</class-name>
  </service> 
  <service>
    <name>Msan Management Module</name>
    <class-name>com.ltln.modules.msan.server.util.cscommunication.MsanMainServer</class-name>
  </service>
  <service>
    <name>Mtosi Northbound Interface Module</name>
    <class-name>com.ltln.modules.ni.mtosi.start.MTMain</class-name>
  </service>
</service-configuration>
